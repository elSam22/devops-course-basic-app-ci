{
  "name": "devops-course-basic-app-ci",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/elSam22/devops-course-basic-app-ci.git",
    "buildspec": "buildspec.yml",
    "gitCloneDepth": 1
  },
  "artifacts": {
    "type": "S3",
    "location": "my-codebuild-artifacts-bucket",
    "packaging": "NONE",
    "name": "nosetests.xml"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:7.0",
    "computeType": "BUILD_GENERAL1_SMALL",
    "environmentVariables": [],
    "privilegedMode": false
  },
  "serviceRole": "arn:aws:iam::<YOUR_ACCOUNT_ID>:role/<CodeBuildRole>",
  "timeoutInMinutes": 20
}